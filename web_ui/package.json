    "scripts": {
        "build": "rsbuild build --environment geti --environment admin",
        "build:admin": "rsbuild build --environment admin",
        "build:all-routes": "rsbuild build --environment dev",
        "build:analyze": "RSDOCTOR=true npm run build",
        "build:geti": "rsbuild build --environment geti",
        "build:rest-openapi-spec": "npx @redocly/cli bundle --dereferenced ../docs/rest_api/openapi.yaml -o public/openapi-spec.json",
        "build:server": "npm run build:server-schema && npm run build:server-combine && npm run build:server-types",
        "build:server-combine": "node ./src/core/server/combine.js",
        "build:server-schema": "swagger-cli bundle -o src/core/server/generated/api-spec.json ../docs/rest_api/openapi_internal.yaml -r",
        "build:server-types": "openapi-typescript src/core/server/generated/api-spec.json --output src/core/server/generated/schemas.ts",
        "cyclic-deps-check": "npx madge --circular src/**/*.ts* tests/**/*.ts* packages/**/*.ts*",
        "find-deadcode": "npx knip",
        "format": "prettier --write --cache \"./src/**/*.{js,ts,jsx,tsx,css,scss,sass}\" \"tests/**/*.ts*\" \"packages/**/*.ts*\"",
        "format:check": "prettier --check --cache \"./src/**/*.{js,ts,jsx,tsx,css,scss,sass}\" \"tests/**/*.ts*\" \"packages/**/*.ts*\"",
        "lint": "eslint \"src/**/*.ts*\" \"tests/**/*.ts*\" --max-warnings 0 && npm run lint --workspaces --if-present",
        "lint:fix": "eslint --fix \"./src/**/*.ts*\" \"tests/**/*.ts*\" && npm run lint:fix --workspaces --if-present",
        "preview": "rsbuild preview",
        "start": "rsbuild dev --environment dev",
        "test:component": "npx playwright test --config=playwright.config.ts",
        "test:component:ui": "npm run test:component -- --ui",
        "test:coverage": "npm run test:unit -- --coverage",
        "test:e2e": "npx playwright test --config=playwright.config.ts --project=e2e",
        "test:unit": "jest --config jest.config.js --watchAll=false",
        "test:unit:watch": "jest --config jest.config.js --watch",
        "type-check": "tsc --noEmit && npm run type-check --workspaces --if-present"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@apidevtools/swagger-cli": "^4.0.4",
        "@eslint/compat": "^1.3.1",
        "@geti/config": "*",
        "@ianvs/prettier-plugin-sort-imports": "^4.5.1",
        "@mjackson/headers": "^0.10.0",
        "@playwright/test": "^1.55.0",
        "@rsbuild/core": "^1.4.14",
        "@rsbuild/plugin-react": "^1.3.5",
        "@rsbuild/plugin-sass": "^1.3.4",
        "@rsbuild/plugin-svgr": "^1.2.2",
        "@rsdoctor/rspack-plugin": "^1.1.11",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.6.4",
        "@testing-library/react": "^16.3.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/adm-zip": "^0.5.7",
        "@types/jest": "^29.5.14",
        "@types/lodash-es": "^4.17.12",
        "@types/node": "^22.15.3",
        "@types/polylabel": "^1.1.3",
        "@types/react": "^19.1.10",
        "@types/react-beautiful-dnd": "^13.1.8",
        "@types/react-dom": "^19.1.7",
        "@types/utif": "^3.0.5",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "adm-zip": "^0.5.16",
